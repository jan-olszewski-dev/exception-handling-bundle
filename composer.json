{
  "name": "jan-olszewski-dev/exception-handling-bundle",
  "description": "Bundle to automatically deal with exceptions in symfony",
  "version": "1.0.1",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/http-kernel": "^6.2",
    "symfony/dependency-injection": "^6.2",
    "symfony/monolog-bundle": "^3.8",
    "symfony/routing": "^6.2",
    "graylog2/gelf-php": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Jolszewski\\ExceptionHandlingBundle\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Jan Olszewski"
    }
  ],
  "extra": {
    "symfony": {
      "require": "^6.2"
    }
  }
}
